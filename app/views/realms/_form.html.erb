<%= render('layouts/errors', :object => @realm ) %>

<%= form_for @realm do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br><br>
  <%= f.label :price %>
  <%= f.number_field :price, :class => 'text_field', :step => 0.01 %>
  <br><br>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <br><br>
  <p><%= f.submit %></p>
<% end %>
